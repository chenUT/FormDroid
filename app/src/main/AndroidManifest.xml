<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.chen.app" >

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:name=".FDApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/SampleTheme.NoActionBar" >
        <activity android:name=".MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SampleTestFormActivity"
            android:label="@string/title_activity_sample_form"
            android:theme="@style/SampleTheme.NoActionBar" >
        </activity>
        <activity
            android:name=".FormHolderActivity"
            android:label="@string/title_activity_form_holder"
            android:theme="@style/SampleTheme.NoActionBar" >
        </activity>
    </application>

</manifest>
